<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Cadastro de aluno</title>
</head>

<body>
  <div>

    <% if(errors.length > 0){ %>
      <div style="color:red">
        <% errors.forEach((erro) => { %>

          <%= erro %><br>

            <%} ) %>
      </div>
      <% } %>

        <% if(success.length > 0){ %>
          <div style="color: green;">
            <% success.forEach((success) => { %>

              <%= success %><br>

                <%} ) %>
          </div>
          <% } %>

  </div>

  <a href="/"><button>Voltar</button></a>
</body>

</html>
